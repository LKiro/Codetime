# Codetime env example
PORT=3000
WS_PATH=/ws
CODETIME_CONCURRENCY_POLICY=exclusive
RATE_HTTP_PER_MIN=1200
RATE_WS_PER_MIN=120
DEV_TOKEN_AUTO=false

# MySQL (optional for MVP; if not set, server uses in-memory store)
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=codetime
DB_PASSWORD=codetime
DB_DATABASE=codetime

# Sessions & OAuth (optional)
SESSION_SECRET=please-change-me
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_CALLBACK_URL=http://localhost:3000/auth/github/callback

# Token hashing pepper (optional but recommended)
TOKEN_PEPPER=
